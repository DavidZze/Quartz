  <SQLQuery>
    <![CDATA[select 
      qt.sched_name       scheduler_name
,     qt.prev_fire_time   prev_fire_time
,     qt.next_fire_time   next_fire_time
,     qt.priority         priority
,     qt.start_time       start_time
,     qt.trigger_state    trigger_state
,     qt.trigger_type     trigger_type
,     qjd.job_name        job_name
,     qjd.job_group       job_group
,     qjd.job_class_name  job_class_name
,     qjd.description     job_description
,     qct.cron_expression job_cron_expression
from qrtz_job_details qjd,
     qrtz_triggers    qt,
     qrtz_cron_triggers qct
where qjd.job_name = qt.job_name
and   qjd.job_group = qt.job_group 
and   qjd.sched_name = qt.sched_name    
and   qct.trigger_name = qt.job_name
and   qct.trigger_group = qt.job_group]]>
  </SQLQuery>
